server:
  port: 8089

eureka:
  instance:
    hostname: localhost
  client:
    registerWithEureka: true # telling the server not to register himself in the service
    fetchRegistry: true
  
zuul:
    prefix: /api
    routes:
      user-DB-Server:
         path: /server/**
         url: http://localhost:8080
      user-client:
           path:/client/**
         url: http://localhost:8070
         
    
    
    
      
     

